{
	"name": "REST_HubspotPHP",
	"properties": {
		"description": "Conexión a API en servidor PHP que consulta a Hubspot evitando la paginación",
		"parameters": {
			"hs_object": {
				"type": "string"
			},
			"hs_properties": {
				"type": "string"
			},
			"hs_associations": {
				"type": "string"
			},
			"hs_hapikey": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "@{concat('https://power-bi-hubspot-connector.nacdcm.com/index.php?hapikey=',linkedService().hs_hapikey,&action=getRecords&object=',linkedService().hs_object,'&properties=',linkedService().hs_properties,'&associations=',linkedService().hs_associations)}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		},
		"connectVia": {
			"referenceName": "IRTrabajoFinal",
			"type": "IntegrationRuntimeReference"
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}